<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_equipo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/campofutbol"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.emiliano.prueba.EquipoActivity"
    tools:showIn="@layout/activity_equipo">


    <ImageButton
        android:id="@+id/btnDef3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/jugador"
        android:background="@null"
        android:layout_alignTop="@+id/btnDef2"
        android:layout_toRightOf="@+id/btnArq"
        android:layout_toEndOf="@+id/btnArq"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp" />
    <ImageButton
        android:id="@+id/btnDef1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:src="@drawable/jugador"
        android:background="@null"
        android:layout_below="@+id/btnArq"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp" />

    <ImageButton
        android:id="@+id/btnDef2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/jugador"
        android:background="@null"
        android:layout_alignTop="@+id/btnDef1"
        android:layout_toLeftOf="@+id/btnArq"
        android:layout_toStartOf="@+id/btnArq"
        android:layout_marginRight="14dp"
        android:layout_marginEnd="14dp" />



    <ImageButton
        android:id="@+id/btnArq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="24dp"
        android:src="@drawable/jugador"
        android:background="@null" />

    <ImageButton
        android:id="@+id/btnDef4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/jugador"
        android:background="@null"
        android:layout_alignTop="@+id/btnDef3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="17dp"
        android:layout_marginEnd="17dp" />

    <ImageButton
        android:id="@+id/btnMed2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/jugador"
        android:background="@null"
        android:paddingTop="100dp"
        android:layout_below="@+id/btnDef2"
        android:layout_toLeftOf="@+id/btnDef3"
        android:layout_toStartOf="@+id/btnDef3" />


    <ImageButton
        android:id="@+id/btnMed1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/jugador"
        android:background="@null"
        android:paddingTop="100dp"
        android:layout_below="@+id/btnDef2"
        android:layout_toRightOf="@+id/btnDef1"
        android:layout_toEndOf="@+id/btnDef1" />

    <ImageButton
        android:id="@+id/btnMed3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/jugador"
        android:background="@null"
        android:paddingTop="100dp"
        android:layout_marginRight="26dp"
        android:layout_marginEnd="26dp"
        android:layout_below="@+id/btnDef4"
        android:layout_alignRight="@+id/btnDef4"
        android:layout_alignEnd="@+id/btnDef4" />


    <ImageButton
        android:id="@+id/btnDel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/jugador"
        android:background="@null"
        android:paddingTop="100dp"
        android:layout_below="@+id/btnMed2"
        android:layout_alignLeft="@+id/btnMed2"
        android:layout_alignStart="@+id/btnMed2" />

    <ImageButton
        android:id="@+id/btnDel3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/jugador"
        android:background="@null"
        android:paddingTop="100dp"
        android:layout_below="@+id/btnMed3"
        android:layout_alignLeft="@+id/btnMed3"
        android:layout_alignStart="@+id/btnMed3" />

    <ImageButton
        android:id="@+id/btnDel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/jugador"
        android:background="@null"
        android:paddingTop="100dp"
        android:layout_below="@+id/btnMed1"
        android:layout_alignLeft="@+id/btnMed1"
        android:layout_alignStart="@+id/btnMed1" />


</RelativeLayout>
